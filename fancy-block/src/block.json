{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/fancy-block",
	"version": "0.1.0",
	"title": "Fancy Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"kicker": {
			"type": "string",
			"default": ""
		},
		"headline": {
			"type": "string",
			"default": ""
		},
		"subdeck": {
			"type": "string",
			"default": ""
		},
		"kickerBgColor": {
			"type": "string",
			"default": "#fff"
		},
		"kickerTextColor": {
			"type": "string",
			"default": "#000"
		},
		"headlineTextColor": {
			"type": "string",
			"default": "#000"
		},
		"subdeckTextColor": {
			"type": "string",
			"default": "#000"
		}
	},
	"textdomain": "fancy-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
